{"body": [
    {"form target='frame' method='POST'": [
        {"fieldset": [
            {"label": [
                {"input": {"type":"color", "name":"infracolor", "value":"#333333"}},
                {"input": {"type":"color", "name":"shadowcolor", "value":"#333333"}},
                {"input": {"type":"color","name":"hypercolor", "value":"#eeeeee" }}
            ]}
        ]},
        {"fieldset":[
            {"label": [
                "motif",
                {"select name='motif'": [
                    {"option": {"value": "none", "childNodes": ["none"]}},
                    {"option": {"value": "square", "childNodes": ["square"]}},
                    {"option": {"value": "pyritohedron", "childNodes": ["pyritohedron"]}},
                    {"option": {"value": "p4octagon", "childNodes": ["p4octagon"], "selected":""}},
                    {"option": {"value": "honeycomb", "childNodes": ["honeycomb"]}},
                    {"option": {"value": "doublesquares", "childNodes": ["doublesquares"]}},
                    {"option": {"value": "alternatetriangles", "childNodes": ["alternatetriangles"]}},
                    {"option": {"value": "threestars", "childNodes": ["threestars"]}}
                ]}
            ]},
            {"label": [
                "strapwork", 
                {"input":{
                    "name":"strapwork","type":"number","min":"0","max":"50","step":"1","value":"10"
                }},
                "px"
            ]},
            {"label": [
                "shells", 
                {"input":{
                    "name":"shells","type":"number","min":"1","max": "50","step":"1","value":"10"
                }}
            ]},
            {"label": [
                "radius", 
                {"input":{
                    "name":"radius","type":"number","min":"1","max": "500","step":"10","value":"50"
                }}
            ]},
            {"label": [
                "shadow-offset-x", 
                {"input":{
                    "name":"shadowxoffset","type":"range","min":"0","max": "100","step":"10","value":"10"
                }}
            ]},
            {"label": [
                "shadow-offset-y", 
                {"input":{
                    "name":"shadowyoffset","type":"range","min":"0","max": "100","step":"10","value":"10"
                }}
            ]},
            {"label": [
                "shadow-blur", 
                {"input":{
                    "name":"shadowblur","type":"range","min":"0","max": "10","step":"0.1","value":"2"
                }}
            ]}
        ]},
        {"fieldset":[
            {"label": [
                "precision", 
                {"input":{
                    "name":"precision","type":"number","min":"2","max": "8","step":"1","value":"6"
                }}
            ]},
            {"label": [
                "overlap", 
                {"input":{
                    "name":"overlap","type":"number","min":"0","max": "0.01","step":".001","value":".001"
                }}
            ]}
        ]}
    ]},
    {"iframe": {
        "name": "frame"
    }}
]}